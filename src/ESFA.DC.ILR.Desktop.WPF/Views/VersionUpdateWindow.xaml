<Window x:Class="ESFA.DC.ILR.Desktop.WPF.Views.VersionUpdateWindow"
        x:Name="versionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Update" Height="200" Width="400" WindowStartupLocation="CenterOwner"
        DataContext="{Binding Version, Source={StaticResource Locator}}" ResizeMode="NoResize"
        Icon="../Resources/Images/DSAppIcon.ico"> 
   
    
</Window>
